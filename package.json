    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "primeicons": "^6.0.1",
    "primereact": "^10.3.1",
    "react": "^18.2.0",
    "react-babylonjs": "^3.1.27",
    "react-chrono": "^2.4.2",
